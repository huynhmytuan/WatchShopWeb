@model IEnumerable<WatchShop.Models.Supplier>

<li class="dropdown">
    <a href="#" class="nav-link" data-toggle="dropdown">Thương Hiệu</a>
    <ul class="dropdown-menu">
        @foreach(var item in Model)
            {
